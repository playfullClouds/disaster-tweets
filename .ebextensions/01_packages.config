packages:
  yum:
    postgresql-devel: []
    postgresql12-devel: []
    amazon-linux-extras: []

commands:
  01_postgres_activate:
    command: "sudo amazon-linux-extras enable postgresql12"
  02_postgres_install:
    command: "sudo yum install -y postgresql12-devel"
  03_update_packages:
    command: "sudo yum update -y"
